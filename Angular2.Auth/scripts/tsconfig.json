{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "../wwwroot/appScripts/"
    },
    "filesGlob": [
        "node_modules/angular2/typings/browser.d.ts"
    ],
    "exclude": [
        "node_modules",
        "typings/main",
        "typings/main.d.ts"
    ]
}
